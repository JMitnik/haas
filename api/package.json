{
    "name": "@haas/api",
    "private": true,
    "version": "0.1.0",
    "workspaces": {
        "nohoist": [
            "@prisma",
            "@prisma/**"
        ]
    },
    "scripts": {
        "start": "nodemon",
        "du": "docker-compose up -d",
        "serve": "ts-node -P tsconfig.json ./src/app.ts",
        "build": "yarn",
        "test": "jest --detectOpenHandles",
        "logs": "",
        "deploy": "echo 'not setup webhook yet'",
        "migrate:create": "npx prisma migrate save --experimental",
        "migrate": "prisma migrate up --experimental",
        "gen": "prisma generate && ts-node ./src/schema.ts",
        "seed": "ts-node --transpile-only src/data/main-seed.ts"
    },
    "dependencies": {
        "@graphql-codegen/cli": "^1.11.2",
        "@graphql-codegen/typescript": "^1.11.2",
        "@graphql-codegen/typescript-resolvers": "^1.11.2",
        "@nexus/schema": "^0.13.1",
        "@prisma/cli": "2.0.0-beta.3",
        "@prisma/client": "2.0.0-beta.2",
        "@types/lodash": "^4.14.149",
        "@types/mjml": "^4.0.4",
        "@types/nodemailer": "^6.4.0",
        "apollo-datasource": "^0.7.0",
        "apollo-datasource-rest": "^0.8.0",
        "apollo-server": "^2.11.0",
        "apollo-server-express": "^2.11.0",
        "apollo-utilities": "^1.3.3",
        "body-parser": "^1.19.0",
        "cloudinary": "^1.21.0",
        "concurrently": "^5.1.0",
        "cors": "^2.8.5",
        "date-fns": "^2.12.0",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "graphql": "^14.5.8",
        "graphql-import": "^1.0.0-beta.1",
        "graphql-upload": "^10.0.0",
        "lodash": "^4.17.15",
        "mjml": "^4.6.2",
        "nexus": "^0.20.0",
        "nexus-prisma": "^0.12.0",
        "nodemailer": "^6.4.6",
        "nodemailer-sendgrid": "^1.0.3",
        "nodemon": "^2.0.3",
        "path": "^0.12.7",
        "pg": "^8.0.2",
        "react-scripts": "^3.4.0",
        "ts-node": "^8.6.2"
    },
    "devDependencies": {
        "@prisma/cli": "2.0.0-beta.3",
        "@types/jest": "^25.2.1",
        "jest": "^26.0.0",
        "nodemon": "^2.0.3",
        "ts-jest": "^25.4.0",
        "typescript": "^3.7.3"
    }
}
