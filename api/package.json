{
    "name": "@haas/api",
    "version": "0.1.0",
    "scripts": {
        "start": "nodemon --exec 'ts-node' --watch src/ src/app.ts",
        "build": "yarn",
        "clean": "prisma reset",
        "clean:prod": "prisma reset -e .env.prod",
        "generate": "prisma deploy && prisma generate -e .env && graphql-codegen",
        "migrate": "prisma deploy --force -f -e .env && prisma generate && graphql-codegen",
        "migrate:prod": "prisma deploy -f -e .env.prod",
        "seed": "yarn migrate && prisma seed",
        "seed:prod": "yarn migrate && prisma seed -e .env.prod",
        "token": "prisma token -e .env",
        "token:prod": "prisma token -e .env.prod"
    },
    "dependencies": {
        "@graphql-codegen/cli": "^1.11.2",
        "@graphql-codegen/typescript": "^1.11.2",
        "@graphql-codegen/typescript-resolvers": "^1.11.2",
        "body-parser": "^1.19.0",
        "dotenv": "^8.2.0",
        "graphql": "^14.5.8",
        "graphql-import": "^0.7.1",
        "graphql-yoga": "^1.18.3",
        "nodemon": "^2.0.2",
        "prisma-binding": "^2.3.16",
        "prisma-client-lib": "^1.34.10",
        "ts-node": "^8.6.2"
    },
    "devDependencies": {
        "typescript": "^3.7.3"
    }
}
