{
    "name": "@haas/api",
    "private": true,
    "version": "0.1.0",
    "workspaces": {
        "nohoist": [
            "@prisma/client"
        ]
    },
    "scripts": {
        "start": "tsnd --exit-child --transpile-only -- ./src/app.ts",
        "dev": "npx nodemon --watch src ./src/app.ts",
        "build": "tsc -p ./tsconfig.json --outDir dist",
        "docker:up": "docker-compose -f docker-compose.local.yml up -d ",
        "kill": "npx kill-port 4000 && npx kill-port 9229 | exit 0",
        "restart": "yarn kill && yarn start",
        "serve": "ts-node -P tsconfig.json ./src/app.ts",
        "test": "jest --detectOpenHandles",
        "migrate:create": "npx prisma migrate save --experimental",
        "migrate": "prisma migrate up --experimental --verbose",
        "gen": "prisma generate && ts-node ./src/config/schema.ts",
        "seed": "ts-node --transpile-only src/data/main-seed.ts"
    },
    "dependencies": {
        "@nexus/schema": "^0.13.1",
        "@prisma/client": "2.3.0",
        "@sentry/node": "^5.21.4",
        "apollo-datasource": "^0.7.0",
        "apollo-datasource-rest": "^0.8.0",
        "apollo-server": "^2.11.0",
        "apollo-server-express": "^2.11.0",
        "apollo-utilities": "^1.3.3",
        "aws-sdk": "^2.739.0",
        "bcrypt": "^5.0.0",
        "body-parser": "^1.19.0",
        "cloudinary": "^1.21.0",
        "color": "3.1.2",
        "concurrently": "^5.1.0",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "cuid": "^2.1.8",
        "date-fns": "^2.12.0",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "graphql": "^14.5.8",
        "graphql-middleware": "^4.0.2",
        "graphql-shield": "^7.3.5",
        "graphql-upload": "^11.0.0",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.15",
        "mjml": "^4.6.2",
        "nodemon": "^2.0.3",
        "path": "^0.12.7",
        "pg": "^8.0.2",
        "ts-is-present": "^1.1.3",
        "ts-node": "^8.6.2",
        "ts-node-dev": "^1.0.0-pre.55",
        "yup": "^0.29.1"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^1.11.2",
        "@graphql-codegen/typescript": "^1.11.2",
        "@graphql-codegen/typescript-resolvers": "^1.11.2",
        "@prisma/cli": "2.3.0",
        "@types/bcrypt": "^3.0.0",
        "@types/color": "3.0.1",
        "@types/cookie-parser": "^1.4.2",
        "@types/cuid": "^1.3.1",
        "@types/jest": "^25.2.1",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/lodash": "^4.14.149",
        "@types/mjml": "^4.0.4",
        "@types/nodemailer": "^6.4.0",
        "@types/twilio": "^2.11.0",
        "@types/yup": "^0.29.3",
        "jest": "^26.5.3",
        "nodemon": "^2.0.3",
        "ts-jest": "^26.4.1",
        "typescript": "^3.7.3"
    },
    "resolutions": {
        "**/**/fs-capacitor": "^6.2.0"
    }
}
