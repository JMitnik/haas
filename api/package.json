{
    "name": "@haas/api",
    "version": "0.1.0",
    "scripts": {
        "start": "nodemon",
        "serve": "ts-node -P tsconfig.json ./src/app.ts",
        "build": "yarn",
        "logs": "",
        "deploy": "echo 'not setup webhook yet'",
        "generate": "prisma deploy && prisma generate -e .env && graphql-codegen",
        "migrate": "prisma deploy --force -f -e .env && prisma generate && graphql-codegen",
        "seed": "yarn migrate && prisma seed",
        "reset": "prisma reset",
        "token": "prisma token -e .env",
        "token:prod": "prisma token -e .prod.env"
    },
    "dependencies": {
        "@graphql-codegen/cli": "^1.11.2",
        "@graphql-codegen/typescript": "^1.11.2",
        "@graphql-codegen/typescript-resolvers": "^1.11.2",
        "@types/lodash": "^4.14.149",
        "@types/mjml": "^4.0.4",
        "@types/nodemailer": "^6.4.0",
        "apollo-datasource": "^0.7.0",
        "apollo-datasource-rest": "^0.8.0",
        "apollo-server": "^2.11.0",
        "apollo-server-express": "^2.11.0",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "graphql": "^14.5.8",
        "graphql-import": "^1.0.0-beta.1",
        "graphql-yoga": "^1.18.3",
        "lodash": "^4.17.15",
        "mjml": "^4.6.2",
        "nodemailer": "^6.4.6",
        "nodemailer-sendgrid": "^1.0.3",
        "nodemon": "^2.0.2",
        "pm2": "^4.2.3",
        "prisma-binding": "^2.3.16",
        "prisma-client-lib": "^1.34.10",
        "ts-node": "^8.6.2"
    },
    "devDependencies": {
        "nodemon": "^2.0.3",
        "typescript": "^3.7.3"
    }
}
