{
  "name": "@haas/api",
  "private": true,
  "version": "0.1.0",
  "workspaces": {
    "nohoist": [
      "@prisma",
      "@prisma/**"
    ]
  },
  "scripts": {
    "start": "tsnd --trace-deprecation --exit-child --transpile-only -- ./src/app.ts",
    "start:debug": "tsnd --inspect=localhost:9229 --exit-child --transpile-only -- ./src/app.ts",
    "dev": "npx nodemon --watch src ./src/app.ts",
    "build": "yarn",
    "docker:up": "docker-compose -f docker-compose.local.yml up -d ",
    "kill": "npx kill-port 4000 && npx kill-port 9229 | exit 0",
    "restart": "yarn kill && yarn start",
    "serve": "ts-node -P tsconfig.json ./src/app.ts",
    "test": "yarn test:integration",
    "test:integration": "jest",
    "test:watch": "jest --watch --maxWorkers=0",
    "migrate:create": "prisma migrate dev --create-only",
    "migrate:baseline": "prisma migrate resolve",
    "migrate": "prisma migrate dev",
    "gen": "prisma generate && ts-node ./src/config/schema.ts",
    "gen:prisma": "prisma generate",
    "seed": "ts-node --transpile-only src/data/main-seed.ts",
    "seed:basic": "ts-node src/scripts/seed.ts",
    "usp:mapSessionScores": "ts-node src/scripts/mapSliderScoreToSession.ts",
    "artillery:seed": "ts-node src/scripts/artillery-seed.ts",
    "artillery:generateUsersPayload": "ts-node src/scripts/generateArtilleryUsersPayload.ts",
    "types": "tsc --noEmit",
    "prisma": "prisma"
  },
