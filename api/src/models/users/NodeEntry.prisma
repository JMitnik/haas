type NodeEntry {
    id: ID! @id
    session: Session! @relation(name: "SessionToNodeEntries")
    relatedNode: QuestionNode
    edgeChild: Edge
    values: [NodeEntryValue] @relation(link: TABLE, name: "NodeEntryToNodeEntryValue", onDelete: CASCADE)
    depth: Int
    creationDate: DateTime! @createdAt
}

type NodeEntryValue {
    id: ID! @id
    textValue: String
    numberValue: Int
    multiValues: [NodeEntryValue]
}
