type NodeEntry {
    id: ID! @id
    session: Session!
    relatedNode: QuestionNode!
    edgeChild: Edge
    values: [NodeEntryValue]
    depth: Int
    creationDate: DateTime! @createdAt
}

type NodeEntryValue {
    id: ID! @id
    textValue: String
    numberValue: Int
    multiValues: [NodeEntryValue]
}
