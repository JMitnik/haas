type Questionnaire {
    id: ID! @id
    customer: Customer! @relation(link: INLINE)
    title: String!
    description: String!
    publicTitle: String
    creationDate: DateTime! @createdAt
    updatedAt: DateTime @updatedAt
    rootQuestion: QuestionNode @relation(link: TABLE, name: "QuestionnaireRootQuestion")
    questions: [QuestionNode] @relation(link: TABLE, name: "QuestionnaireQuestions")
    leafs: [LeafNode!] @relation(link: TABLE, name: "QuestionnaireLeafs")
}
